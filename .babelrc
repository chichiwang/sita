{
  "presets": [
    "stage-2",
    "react",
    "flow"
  ],
  "plugins": [
    ["module-resolver", {
      "root": ["./src"]
    }],
    "transform-es2015-duplicate-keys",
    "transform-es2015-modules-commonjs",
    "syntax-dynamic-import",
  ],
  "env": {
    "server": {
      "plugins": ["dynamic-import-node"]
    }
  }
}
